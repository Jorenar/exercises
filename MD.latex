\section{Zadanie 4a}

Przy pomocy funkcji tworzących znaleźć jawny wzór na $n$-ty wyraz ciągu $(a_n)$ jeśli:
\[ a_0 = 1, \quad a_1 = 1, \quad a_{n+2} = a_{n+1} + 2a_n \]

\subsection*{Rozwiązanie}

\begin{align*}
   A(x) &= \sum_{n=0}^\infty a_n x^n = a_0 + a_1 x + \sum_{n=2}^\infty a_n x^n
   =\\ &=
   1 + x + \sum_{n=2}^\infty a_n x^n = 1 + x + \sum_{n=0}^\infty a_{n+2} x^{n+2}
   =\\ &=
   1 + x + \sum_{n=0}^\infty (a_{n+1} + 2a_n) x^{n+2}
   =\\ &=
   1 + x + \sum_{n=0}^\infty a_{n+1} x^{n+2} + \sum_{n=0}^\infty 2a_n x^{n+2}
   =\\ &=
   1 + x
   + x \underbrace{\sum_{n=0}^\infty a_{n+1} x^{n+1}}_{A(x) - a_0}
   + 2x^2 \underbrace{\sum_{n=0}^\infty a_n x^n}_{A(x)}
   =\\ &=
   1 + x + x(A(x) - a_0) + 2x^2 A(x)
   =\\ &=
   1 + x + xA(x) - a_0 x + 2x^2 A(x)
   =\\ &=
   1 + x + xA(x) - x + 2x^2 A(x)
   =\\ &=
   1 + xA(x) + 2x^2 A(x)
   \implies
   A(x) \cdot (1 - x - 2x^2) = 1
   \underarrow[\implies][\uparrow]{\footnotesize\textrm{\textit{dla $1-x-2x^2=0$ lewa strona i tak nie byłaby prawdą}}}
   A(x) = \frac{1}{1-x-2x^2}
\end{align*}

\begin{equation*}
   A(x) = \frac{1}{1-x-2x^2} = \frac{1}{(1+x)(1-2x)} = \frac{\beta}{1+x} + \frac{\gamma}{1-2x}
   \implies
   1 = \beta \cdot (1-2x) + \gamma \cdot (1+x)
\end{equation*}
\begin{equation*}
   \begin{cases}
      1 = \beta \cdot (1+2) \for x = -1 \\
      1 = \frac{3}{2} \gamma \for x = \frac{1}{2}
   \end{cases}
   \implies
   \begin{cases}
      \beta = \frac{1}{3} \\
      \gamma = \frac{2}{3}
   \end{cases}
   \implies
   A(x) = \frac{1}{3} \cdot \frac{1}{1+x} + \frac{2}{3} \cdot \frac{1}{1-2x}
\end{equation*}

\begin{align*}
   A(x) &= \frac{1}{3} \cdot \frac{1}{1+x} + \frac{2}{3} \cdot \frac{1}{1-2x}
   =\\ &=
   \frac{1}{3} \sum_{n=0}^\infty (-1)^n x^n + \frac{2}{3} \sum_{n=0}^\infty 2^n x^n
   =\\ &=
   \sum_{n=0}^\infty \left( \frac{1}{3} \cdot (-1)^n + \frac{2}{3} \cdot 2^n \right) x^n
   \implies
   a_n = \frac{1}{3} \cdot (-1)^n + \frac{2}{3} \cdot 2^n
\end{align*}

\begin{equation*}
   a_n = \frac{(-1)^n + 2^{n+1}}{3}
\end{equation*}

