   \left\{ \begin{aligned}
      x &\equiv 1 \pmod{9}    && \textrm{\tiny{(1)}} \\
      x &\equiv 5 \pmod{7}    && \textrm{\tiny{(2)}} \\
      x &\equiv 40 \pmod{49}  && \textrm{\tiny{(3)}} \\
      x &\equiv 1 \pmod{3}    && \textrm{\tiny{(4)}} \\
   \end{aligned} \right.
\end{equation*}
\begin{gather*}
   \textrm{\tiny{(1)}} \implies x = 9k + 1 = 3(3k) + 1 \implies x \equiv 1 \pmod{3}
   \implies \textrm{można pominąć {\tiny (4)} w układzie} \\
   %
   \textrm{\tiny{(3)}} \implies x = 49k + 40 = 7(7k+5) + 5 \implies x \equiv 5 \pmod{7}
   \implies \textrm{można pominąć {\tiny (2)} w układzie} \\
\end{gather*}
\begin{equation*}
   \left\{ \begin{aligned}
      x &\equiv 1 \pmod{9} \\
      x &\equiv 40 \pmod{49} \\
   \end{aligned} \right.
   \implies
   x \equiv 334 \pmod{441}
\end{equation*}

