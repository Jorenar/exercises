\section{Zadanie 18a}

Załóżmy, że $N = pq$, gdzie $p, q$ to różne liczby pierwsze.\\
Pokazać, że znajomość wartości $\varphi(N)$ pozwala znaleźć rozkład liczby $N$.

\subsection*{Rozwiązanie}
\begin{equation*}
   N = pq, \quad \varphi(N) = A
\end{equation*}

\begin{equation*}
   A = \varphi(N) = \varphi(p) \varphi(q) = (p-1)(q-1) = pq -p - q + 1 \implies p+q = \varphi(N) - pq - 1
\end{equation*}

\begin{equation*}
   \begin{aligned}
      \left\{ \begin{aligned}
         &p+q = N-A+1 \\
         &pq  = N \implies q = \frac{N}{p}
      \end{aligned} \right.
      &\implies p + \frac{N}{p} = N-A+1 \implies \\
      &\implies p^2 + N = Np - Ap + p \implies \\
      &\implies p^2 - p(N+A-1) + N = 0
   \end{aligned}
\end{equation*}

Równanie kwadratowe $\implies$ da się odnaleźć liczbę $p$ \quad\qed

\section{Zadanie}
Rozwiązać równanie $x^2 + 4x + 3 = 0$ w $\Z_6$ oraz $\Z_{13}$.

\subsection*{Rozwiązanie}

\vspace{-2em}\begin{align*}
   x^2 + 4x + 3 &= 0 \\
   x^2 + 4x + 4 &= 1 \\
   (x+2)^2      &= 1 \\
   x+2          &= \sqrt{1}
\end{align*}
$\Z_6$:
\begin{equation*}
   \begin{aligned}
      \sqrt{1} \in \{ -5, -1, 1, 5 \} & \overset{\bmod 6}{\implies}
      \sqrt{1} \in \{ 1, 5, 1, 5 \} \implies
      \sqrt{1} \in \{ 1, 5 \} \implies \\
      &\implies
      x+2      \in \{ 1, 5 \} \implies
      \Big| x        \in \{ -1, 3 \} \Big| \implies
      x        \in \{ 5, 3 \} \implies \\
      &\implies \mathbf{x_1 = 3, \ x_2 = 5}
   \end{aligned}
\end{equation*}
$\Z_{13}$:
\begin{equation*}
   \begin{aligned}
      \sqrt{1} \in \{ -12, -1, 1, 12 \} & \overset{\bmod 6}{\implies}
      \sqrt{1} \in \{ 1, 12, 1, 12 \} \implies
      \sqrt{1} \in \{ 1, 12 \} \implies \\
      &\implies
      x+2      \in \{ 1, 12 \} \implies
      \Big| x        \in \{ -1, 10 \} \Big| \implies
      x        \in \{ 12, 3 \} \implies \\
      &\implies \mathbf{x_1 = 10, \ x_2 = 12}
   \end{aligned}
\end{equation*}

